	pages = {1474--1493},
	file = {Texte intégral:/home/pierre/snap/zotero-snap/common/Zotero/storage/GIVCG7DE/Cosmo et al. - 2022 - 3D Shape Analysis Through a Quantum Lens the Aver.pdf:application/pdf},
