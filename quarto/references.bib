	urldate = {2022-12-02},
	journal = {Technical Innovations \& Patient Support in Radiation Oncology},
	author = {Penninkhof, Joan and Fremeijer, Kimm and Offereins-van Harten, Kirsten and van Wanrooij, Cynthia and Quint, Sandra and Kunnen, Britt and Hoffmans-Holtzer, Nienke and Swaak, Annemarie and Baaijens, Margreet and Dirkx, Maarten},
	month = mar,
