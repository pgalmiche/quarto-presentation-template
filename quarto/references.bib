@article{pokrassPartialShapeMatching2013,
	title = {Partial {Shape} {Matching} {Without} {Point}-{Wise} {Correspondence}},
	volume = {6},
	issn = {1004-8979, 2079-7338},
	url = {http://global-sci.org/intro/article_detail/nmtma/5901.html},
	doi = {10.4208/nmtma.2013.mssvm12},
	abstract = {Partial similarity of shapes is a challenging problem arising in many important applications in computer vision, shape analysis, and graphics, e.g. when one has to deal with partial information and acquisition artifacts. The problem is especially hard when the underlying shapes are non-rigid and are given up to a deformation. Partial matching is usually approached by computing local descriptors on a pair of shapes and then establishing a point-wise non-bijective correspondence between the two, taking into account possibly different parts. In this paper, we introduce an alternative correspondence-less approach to matching fragments to an entire shape undergoing a non-rigid deformation. We use region-wise local descriptors and optimize over the integration domains on which the integral descriptors of the two parts match. The problem is regularized using the Mumford-Shah functional. We show an efﬁcient discretization based on the Ambrosio-Tortorelli approximation generalized to triangular point clouds and meshes, and present experiments demonstrating the success of the proposed method.},
	language = {en},
	number = {1},
	urldate = {2023-01-10},
	journal = {Numerical Mathematics: Theory, Methods and Applications},
	author = {Pokrass, Jonathan},
	month = jun,
	year = {2013},
	pages = {223--244},
	file = {Pokrass - 2013 - Partial Shape Matching Without Point-Wise Correspo.pdf:/home/pierre/snap/zotero-snap/common/Zotero/storage/TAKFRB6G/Pokrass - 2013 - Partial Shape Matching Without Point-Wise Correspo.pdf:application/pdf},
}

@article{bronsteinPartialSimilarityObjects,
	title = {Partial similarity of objects and text sequences},
	abstract = {Similarity is one of the most important abstract concepts in the human perception of the world. In computer vision, numerous applications deal with comparing objects observed in a scene with some a priori known patterns. Often, it happens that while two objects are not similar, they have large similar parts, that is, they are partially similar. Here, we present a novel approach to quantify this semantic deﬁnition of partial similarity using the notion of Pareto optimality. We exemplify our approach on the problems of recognizing non-rigid objects and analyzing text sequences.},
	language = {en},
	author = {Bronstein, Alexander M and Bronstein, Michael M and Bruckstein, Alfred M and Kimmel, Ron},
	file = {Bronstein et al. - Partial similarity of objects and text sequences.pdf:/home/pierre/snap/zotero-snap/common/Zotero/storage/ZB7ZG9ZP/Bronstein et al. - Partial similarity of objects and text sequences.pdf:application/pdf},
}

@article{cosmoConsistentPartialMatching2017,
	title = {Consistent {Partial} {Matching} of {Shape} {Collections} via {Sparse} {Modeling}: {Consistent} {Partial} {Matching} of {Shape} {Collections}},
	volume = {36},
	issn = {01677055},
	shorttitle = {Consistent {Partial} {Matching} of {Shape} {Collections} via {Sparse} {Modeling}},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/cgf.12796},
	doi = {10.1111/cgf.12796},
	abstract = {Recent efforts in the area of joint object matching approach the problem by taking as input a set of pairwise maps, which are then jointly optimized across the whole collection so that certain accuracy and consistency criteria are satisﬁed. One natural requirement is cycle-consistency—namely the fact that map composition should give the same result regardless of the path taken in the shape collection. In this paper, we introduce a novel approach to obtain consistent matches without requiring initial pairwise solutions to be given as input. We do so by optimizing a joint measure of metric distortion directly over the space of cycle-consistent maps; in order to allow for partially similar and extra-class shapes, we formulate the problem as a series of quadratic programs with sparsity-inducing constraints, making our technique a natural candidate for analysing collections with a large presence of outliers. The particular form of the problem allows us to leverage results and tools from the ﬁeld of evolutionary game theory. This enables a highly efﬁcient optimization procedure which assures accurate and provably consistent solutions in a matter of minutes in collections with hundreds of shapes.},
	language = {en},
	number = {1},
	urldate = {2022-12-05},
	journal = {Computer Graphics Forum},
	author = {Cosmo, L. and Rodolà, E. and Albarelli, A. and Mémoli, F. and Cremers, D.},
	month = jan,
	year = {2017},
	pages = {209--221},
	file = {Cosmo et al. - 2017 - Consistent Partial Matching of Shape Collections v.pdf:/home/pierre/snap/zotero-snap/common/Zotero/storage/HFY9P6F5/Cosmo et al. - 2017 - Consistent Partial Matching of Shape Collections v.pdf:application/pdf},
}

@article{sahilliogluPartial3DCorrespondence2014,
	title = {Partial 3-{D} {Correspondence} from {Shape} {Extremities}: {Partial} 3-{D} {Correspondence} from {Shape} {Extremities}},
	volume = {33},
	issn = {01677055},
	shorttitle = {Partial 3-{D} {Correspondence} from {Shape} {Extremities}},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/cgf.12278},
	doi = {10.1111/cgf.12278},
	abstract = {We present a 3D correspondence method to match the geometric extremities of two shapes which are partially isometric. We consider the most general setting of the isometric partial shape correspondence problem, in which shapes to be matched may have multiple common parts at arbitrary scales as well as parts that are not similar. Our rank-and-vote-and-combine (RAVAC) algorithm identiﬁes and ranks potentially correct matches by exploring the space of all possible partial maps between coarsely sampled extremities. The qualiﬁed top-ranked matchings are then subjected to a more detailed analysis at a denser resolution and assigned with conﬁdence values that accumulate into a vote matrix. A minimum weight perfect matching algorithm is ﬁnally iterated to combine the accumulated votes into an optimal (partial) mapping between shape extremities, which can further be extended to a denser map. We test the performance of our method on several datasets and benchmarks in comparison with state of the art.},
	language = {en},
	number = {6},
	urldate = {2023-01-09},
	journal = {Computer Graphics Forum},
	author = {Sahillioğlu, Y. and Yemez, Y.},
	month = sep,
	year = {2014},
	pages = {63--76},
	file = {Sahillioğlu et Yemez - 2014 - Partial 3-D Correspondence from Shape Extremities.pdf:/home/pierre/snap/zotero-snap/common/Zotero/storage/7Y9N3JXW/Sahillioğlu et Yemez - 2014 - Partial 3-D Correspondence from Shape Extremities.pdf:application/pdf},
}

