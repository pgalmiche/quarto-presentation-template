	author = {Takano, Shoko and Omura, Motoko and Suzuki, Ryoko and Tayama, Yumiko and Matsui, Kengo and Hashimoto, Harumitsu and Hongo, Hideyuki and Nagata, Hironori and Tanaka, Kumiko and Hata, Masaharu and Inoue, Tomio},
	month = oct,
