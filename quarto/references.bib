	file = {Melzi et al. - 2018 - Discrete Time Evolution Process Descriptor for Sha.pdf:/home/pierre/snap/zotero-snap/common/Zotero/storage/CRSAST65/Melzi et al. - 2018 - Discrete Time Evolution Process Descriptor for Sha.pdf:application/pdf},
}

@misc{zhouAdjointRigidTransform2023,
