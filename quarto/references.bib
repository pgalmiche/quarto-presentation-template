	doi = {10.1111/cgf.14084},
	abstract = {In this paper, we propose a novel method, which we call CONSISTENT ZOOMOUT, for efﬁciently reﬁning correspondences among deformable 3D shape collections, while promoting the resulting map consistency. Our formulation is closely related to a recent unidirectional spectral reﬁnement framework, but naturally integrates map consistency constraints into the reﬁnement. Beyond that, we show further that our formulation can be adapted to recover the underlying isometry among near-isometric shape collections with a theoretical guarantee, which is absent in the other spectral map synchronization frameworks. We demonstrate that our method improves the accuracy compared to the competing methods when synchronizing correspondences in both near-isometric and heterogeneous shape collections, but also signiﬁcantly outperforms the baselines in terms of map consistency.},
	language = {en},
	number = {5},
