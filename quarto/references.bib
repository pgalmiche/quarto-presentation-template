	keywords = {Computer Graphics I.3.5: Computational Geometry and Object Modeling, Computer Graphics I.3.7: Three-Dimensional Graphics and Realism},
	pages = {337--346},
	file = {Snapshot:/home/pierre/snap/zotero-snap/common/Zotero/storage/HKUIJPF4/j.1467-8659.2009.01373.html:text/html},
}

@article{kurtekLandmarkGuidedElastic2013,
	title = {Landmark‐{Guided} {Elastic} {Shape} {Analysis} of {Spherically}‐{Parameterized} {Surfaces}},
	volume = {32},
	issn = {0167-7055, 1467-8659},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/cgf.12063},
	doi = {10.1111/cgf.12063},
	abstract = {Abstract
            We argue that full surface correspondence (registration) and optimal deformations (geodesics) are two related problems and propose a framework that solves them simultaneously. We build on the Riemannian shape analysis of anatomical and star‐shaped surfaces of Kurtek et al. and focus on articulated complex shapes that undergo elastic deformations and that may contain missing parts. Our core contribution is the re‐formulation of Kurtek et al.'s approach as a constrained optimization over all possible re‐parameterizations of the surfaces, using a sparse set of corresponding landmarks. We introduce a landmark‐constrained basis, which we use to numerically solve this optimization and therefore establish full surface registration and geodesic deformation between two surfaces. The length of the geodesic provides a measure of dissimilarity between surfaces. The advantages of this approach are: (1) simultaneous computation of full correspondence and geodesic between two surfaces, given a sparse set of matching landmarks (2) ability to handle more comprehensive deformations than nearly isometric, and (3) the geodesics and the geodesic lengths can be further used for symmetrizing 3D shapes and for computing their statistical averages. We validate the framework on challenging cases of large isometric and elastic deformations, and on surfaces with missing parts. We also provide multiple examples of averaging and symmetrizing 3D models.},
	language = {en},
	number = {2pt4},
	urldate = {2024-08-01},
	journal = {Computer Graphics Forum},
	author = {Kurtek, Sebastian and Srivastava, Anuj and Klassen, Eric and Laga, Hamid},
	month = may,
	year = {2013},
	pages = {429--438},
	file = {Kurtek et al. - 2013 - Landmark‐Guided Elastic Shape Analysis of Spherica.pdf:/home/pierre/snap/zotero-snap/common/Zotero/storage/NKRPWV4N/Kurtek et al. - 2013 - Landmark‐Guided Elastic Shape Analysis of Spherica.pdf:application/pdf},
}

@article{ovsjanikovExplorationContinuousVariability2011a,
	title = {Exploration of continuous variability in collections of {3D} shapes},
	volume = {30},
	issn = {0730-0301},
	url = {https://doi.org/10.1145/2010324.1964928},
	doi = {10.1145/2010324.1964928},
	abstract = {As large public repositories of 3D shapes continue to grow, the amount of shape variability in such collections also increases, both in terms of the number of different classes of shapes, as well as the geometric variability of shapes within each class. While this gives users more choice for shape selection, it can be difficult to explore large collections and understand the range of variations amongst the shapes. Exploration is particularly challenging for public shape repositories, which are often only loosely tagged and contain neither point-based nor part-based correspondences. In this paper, we present a method for discovering and exploring continuous variability in a collection of 3D shapes without correspondences. Our method is based on a novel navigation interface that allows users to explore a collection of related shapes by deforming a base template shape through a set of intuitive deformation controls. We also help the user to select the most meaningful deformations using a novel technique for learning shape variability in terms of deformations of the template. Our technique assumes that the set of shapes lies near a low-dimensional manifold in a certain descriptor space, which allows us to avoid establishing correspondences between shapes, while being rotation and scaling invariant. We present results on several shape collections taken directly from public repositories.},
	number = {4},
	urldate = {2024-08-01},
	journal = {ACM Trans. Graph.},
	author = {Ovsjanikov, Maks and Li, Wilmot and Guibas, Leonidas and Mitra, Niloy J.},
	month = jul,
	year = {2011},
	pages = {33:1--33:10},
}

@article{grenanderComputationalAnatomyEmerging1998,
	title = {Computational anatomy: an emerging discipline},
	volume = {56},
	issn = {0033-569X, 1552-4485},
	shorttitle = {Computational anatomy},
	url = {https://www.ams.org/qam/1998-56-04/S0033-569X-1998-1668732-7/},
	doi = {10.1090/qam/1668732},
	abstract = {This paper studies mathematical methods in the emerging new discipline of Computational Anatomy. Herein we formalize the Brown/Washington University model of anatomy following the global pattern theory introduced in [1, 2], in which anatomies are represented as deformable templates, collections of 0, 1, 2, 3-dimensional manifolds. Typical structure is carried by the template with the variabilities accommodated via the application of random transformations to the background manifolds. The anatomical model is a quadruple \${\textbackslash}left ( {\textbackslash}Omega , H, I, P {\textbackslash}right )\$, the background space \${\textbackslash}Omega {\textbackslash}dot = \{U\_{\textbackslash}alpha \}\{M\_{\textbackslash}alpha \}\$ of 0, 1, 2, 3-dimensional manifolds, the set of diffeomorphic transformations on the background space \$\{H\} : {\textbackslash}Omega {\textbackslash}leftrightarrow {\textbackslash}Omega\$, the space of idealized medical imagery \$I\$, and \$P\$ the family of probability measures on \$H\$. The group of diffeomorphic transformations \$H\$ is chosen to be rich enough so that a large family of shapes may be generated with the topologies of the template maintained. For normal anatomy one deformable template is studied, with \${\textbackslash}left ( {\textbackslash}Omega , H, I {\textbackslash}right )\$ corresponding to a homogeneous space [3], in that it can be completely generated from one of its elements, \$I = \{HI\_\{temp\}\}, \{I\_\{temp\}\} {\textbackslash}in I\$. For disease, a family of templates \$\{U\_{\textbackslash}alpha \}I\_\{temp\}{\textasciicircum}{\textbackslash}alpha\$ are introduced of perhaps varying dimensional transformation classes. The complete anatomy is a collection of homogeneous spaces \$\{I\_\{total\}\} = \{U\_{\textbackslash}alpha \}{\textbackslash}left ( \{I{\textasciicircum}{\textbackslash}alpha \}, \{H{\textasciicircum}{\textbackslash}alpha \} {\textbackslash}right )\$.},
	language = {en},
	number = {4},
	urldate = {2024-08-01},
	journal = {Quarterly of Applied Mathematics},
	author = {Grenander, Ulf and Miller, Michael I.},
	year = {1998},
	pages = {617--694},
	file = {Full Text PDF:/home/pierre/snap/zotero-snap/common/Zotero/storage/Z6ISLM85/Grenander et Miller - 1998 - Computational anatomy an emerging discipline.pdf:application/pdf},
}

@inproceedings{jermynElasticShapeMatching2012,
