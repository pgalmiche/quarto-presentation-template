@article{ganapathi-subramanianStableRegionCorrespondences2016,
	title = {Stable {Region} {Correspondences} {Between} {Non}‐{Isometric} {Shapes}},
	volume = {35},
	issn = {0167-7055, 1467-8659},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/cgf.12969},
	doi = {10.1111/cgf.12969},
	abstract = {We consider the problem of ﬁnding meaningful correspondences between 3D models that are related but not necessarily very similar. When the shapes are quite different, a point-to-point map is not always appropriate, so our focus in this paper is a method to build a set of correspondences between shape regions or parts. The proposed approach exploits a variety of feature functions on the shapes and makes use of the key observation that points in matching parts have similar ranks in the sorting of the corresponding feature values. Our algorithm proceeds in two steps. We ﬁrst build an afﬁnity matrix between points on the two shapes, based on feature rank similarity over many feature functions. We then deﬁne a notion of stability of a pair of regions, with respect to this afﬁnity matrix, obtained as a ﬁxed point of a nonlinear operator. Our method yields a family of corresponding maximally stable regions between the two shapes that can be used to deﬁne shape parts. We observe that this is an instance of the biclustering problem and that it is related to solving a constrained maximal eigenvalue problem. We provide an algorithm to solve this problem that mimics the power method. We show the robustness of its output to noisy input features as well its convergence properties. The obtained part correspondences are shown to be almost perfect matches in the isometric case, and also semantically appropriate even in non-isometric cases. We provide numerous examples and applications of this technique, for example to sharpening correspondences in traditional shape matching algorithms.},
	language = {en},
	number = {5},
	urldate = {2024-08-06},
	journal = {Computer Graphics Forum},
	author = {Ganapathi‐Subramanian, V. and Thibert, B. and Ovsjanikov, M. and Guibas, L.},
	month = aug,
	year = {2016},
	pages = {121--133},
	file = {Ganapathi‐Subramanian et al. - 2016 - Stable Region Correspondences Between Non‐Isometri.pdf:/home/pierre/snap/zotero-snap/common/Zotero/storage/VHDA8YPQ/Ganapathi‐Subramanian et al. - 2016 - Stable Region Correspondences Between Non‐Isometri.pdf:application/pdf},
