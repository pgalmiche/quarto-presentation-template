	author = {Wang, Shuo Er and Sun, Yi di and Zhao, Song Jiao and Wei, Feng and Yang, Gong},
	month = sep,
	year = {2019},
	pmid = {31731341},
	keywords = {Adult, Aged, Breast Neoplasms, Female, Humans, Middle Aged, Mastectomy, Segmental, Radiotherapy, Adjuvant, radiotherapy, breast conserving surgery (BCS), Combined Modality Therapy, mastectomy, Mastectomy, Neoplasm Staging, Prognosis, Propensity Score, propensity score matching (PSM), Proportional Hazards Models, Risk Factors, SEER Program, Triple Negative Breast Neoplasms, triple-negative breast cancer (TNBC), United States},
