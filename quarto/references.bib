	pmid = {27593113},
	keywords = {Adult, Aged, Baseline, Breast, Breast Neoplasms, Female, Humans, Intra-fraction motion, Middle Aged, Motion, Optical tracking system, Radiometry, Radiotherapy Planning, Computer-Assisted, Radiotherapy, Intensity-Modulated, Respiration, Respiratory motion, Tomotherapy},
