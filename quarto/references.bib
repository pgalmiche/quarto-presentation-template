	file = {Snapshot:/home/pierre/snap/zotero-snap/common/Zotero/storage/L9C34LLP/cgf.html:text/html;Version soumise:/home/pierre/snap/zotero-snap/common/Zotero/storage/5BPLIP3B/Melzi et al. - 2018 - Localized Manifold Harmonics for Spectral Shape An.pdf:application/pdf},
