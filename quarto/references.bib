	year = {2013},
	pages = {849--856},
	file = {Wang et al. - 2013 - Image Co-segmentation via Consistent Functional Ma.pdf:/home/pierre/snap/zotero-snap/common/Zotero/storage/5G6J556C/Wang et al. - 2013 - Image Co-segmentation via Consistent Functional Ma.pdf:application/pdf},
