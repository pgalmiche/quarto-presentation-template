	keywords = {Breast Neoplasms, Female, Heart, Humans, Lung, Proton Therapy, Radiation Injuries, Radiography, Radiotherapy Dosage, Radiotherapy Planning, Computer-Assisted, Radiotherapy, Conformal, Radiotherapy, Intensity-Modulated, Tumor Burden},
	pages = {685--697},
}

@article{lomaxPotentialRoleIntensitymodulated2003,
	title = {Potential role of intensity-modulated photons and protons in the treatment of the breast and regional nodes},
	volume = {55},
	issn = {0360-3016},
