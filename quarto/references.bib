	author = {Alderliesten, Tanja and Heemsbergen, Wilma D. and Betgen, Anja and Topolnjak, Rajko and Elkhuizen, Paula H.M. and van Vliet-Vroegindeweij, Corine and Remeijer, Peter},
	month = may,
	year = {2018},
	pmid = {33458392},
	pmcid = {PMC7807602},
	keywords = {Breast cancer, Breast-conserving surgery, Cone-beam computed tomography, Radiation therapy, Shape variability},
	pages = {71--76},
	file = {PubMed Central Full Text PDF:/home/pierre/snap/zotero-snap/common/Zotero/storage/VVBTTFF8/Alderliesten et al. - 2018 - Breast-shape changes during radiation therapy afte.pdf:application/pdf},
