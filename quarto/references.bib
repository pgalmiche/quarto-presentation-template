@inproceedings{vestnerProductManifoldFilter2017,
	address = {Honolulu, HI},
	title = {Product {Manifold} {Filter}: {Non}-rigid {Shape} {Correspondence} via {Kernel} {Density} {Estimation} in the {Product} {Space}},
	isbn = {978-1-5386-0457-1},
