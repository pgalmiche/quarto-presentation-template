	pages = {1--17},
	file = {Texte intégral:/home/pierre/snap/zotero-snap/common/Zotero/storage/NPUNTHVE/Ren et al. - 2020 - MapTree recovering multiple solutions in the spac.pdf:application/pdf},
}

@article{huangStabilityFunctionalMaps2018,
	title = {On the {Stability} of {Functional} {Maps} and {Shape} {Difference} {Operators}: {On} the {Stability} of {Functional} {Maps} and {Shape} {Difference} {Operators}},
