@article{charonVarifoldRepresentationNonoriented2013a,
	title = {The varifold representation of non-oriented shapes for diffeomorphic registration},
	volume = {6},
	issn = {1936-4954},
	url = {http://arxiv.org/abs/1304.6108},
	doi = {10.1137/130918885},
	abstract = {In this paper, we address the problem of orientation that naturally arises when representing shapes like curves or surfaces as currents. In the ﬁeld of computational anatomy, the framework of currents has indeed proved very eﬃcient to model a wide variety of shapes. However, in such approaches, orientation of shapes is a fundamental issue that can lead to several drawbacks in treating certain kind of datasets. More speciﬁcally, problems occur with structures like acute pikes because of canceling eﬀects of currents or with data that consists in many disconnected pieces like ﬁber bundles for which currents require a consistent orientation of all pieces. As a promising alternative to currents, varifolds, introduced in the context of geometric measure theory by F. Almgren, allow the representation of any non-oriented manifold (more generally any non-oriented rectiﬁable set). In particular, we explain how varifolds can encode numerically non-oriented objects both from the discrete and continuous point of view. We show various ways to build a Hilbert space structure on the set of varifolds based on the theory of reproducing kernels. We show that, unlike the currents’ setting, these metrics are consistent with shape volume (theorem 4.1) and we derive a formula for the variation of metric with respect to the shape (theorem 4.2). Finally, we propose a generalization to non-oriented shapes of registration algorithms in the context of Large Deformations Metric Mapping (LDDMM), which we detail with a few examples in the last part of the paper.},
	language = {en},
	number = {4},
	urldate = {2024-07-31},
	journal = {SIAM Journal on Imaging Sciences},
	author = {Charon, Nicolas and Trouvé, Alain},
	month = jan,
	year = {2013},
	note = {arXiv:1304.6108 [cs, math]},
	keywords = {Computer Science - Computer Vision and Pattern Recognition, Mathematics - Differential Geometry, Computer Science - Computational Geometry},
	pages = {2547--2580},
	annote = {Comment: 33 pages, 10 figures},
	file = {Charon et Trouvé - 2013 - The varifold representation of non-oriented shapes.pdf:/home/pierre/snap/zotero-snap/common/Zotero/storage/8F5H2R2C/Charon et Trouvé - 2013 - The varifold representation of non-oriented shapes.pdf:application/pdf},
