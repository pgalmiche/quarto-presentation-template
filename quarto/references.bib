	pages = {1--6},
	file = {Bronstein et Bronstein - 2008 - Not only size matters Regularized partial matchin.pdf:/home/pierre/snap/zotero-snap/common/Zotero/storage/IQ47EB9B/Bronstein et Bronstein - 2008 - Not only size matters Regularized partial matchin.pdf:application/pdf},
}

@inproceedings{wangImageCosegmentationConsistent2013,
	address = {Sydney, Australia},
	title = {Image {Co}-segmentation via {Consistent} {Functional} {Maps}},
	isbn = {978-1-4799-2840-8},
