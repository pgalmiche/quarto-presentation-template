@misc{antonsantiPartialMatchingSpace2021,
	title = {Partial {Matching} in the {Space} of {Varifolds}},
	url = {http://arxiv.org/abs/2103.12441},
	abstract = {In computer vision and medical imaging, the problem of matching structures ﬁnds numerous applications from automatic annotation to data reconstruction. The data however, while corresponding to the same anatomy, are often very different in topology or shape and might only partially match each other. We introduce a new asymmetric data dissimilarity term for various geometric shapes like sets of curves or surfaces. This term is based on the Varifold shape representation and assesses the embedding of a shape into another one without relying on correspondences between points. It is designed as data attachment for the Large Deformation Diffeomorphic Metric Mapping (LDDMM) framework, allowing to compute meaningful deformation of one shape onto a subset of the other. Registrations are illustrated on sets of synthetic 3D curves, real vascular trees and livers’ surfaces from two different modalities: Computed Tomography (CT) and Cone Beam Computed Tomography (CBCT). All experiments show that this data dissimilarity term leads to coherent partial matching despite the topological differences.},
	language = {en},
	urldate = {2022-08-04},
	publisher = {arXiv},
	author = {Antonsanti, Pierre-Louis and Glaunès, Joan and Benseghir, Thomas and Jugnon, Vincent and Kaltenmark, Irène},
	month = mar,
	year = {2021},
	note = {arXiv:2103.12441 [cs, math]},
	keywords = {58Axx, Computer Science - Computer Vision and Pattern Recognition, Mathematics - Differential Geometry},
	annote = {Comment: 12 pages, 3 figures, The 27th international conference on Information Processing in Medical Imaging (June, 2021)},
	annote = {Comment: 12 pages, 3 figures, The 27th international conference on Information Processing in Medical Imaging (June, 2021)},
	file = {Antonsanti et al. - 2021 - Partial Matching in the Space of Varifolds.pdf:/home/pierre/snap/zotero-snap/common/Zotero/storage/DD9LTZYM/Antonsanti et al. - 2021 - Partial Matching in the Space of Varifolds.pdf:application/pdf;arXiv Fulltext PDF:/home/pierre/snap/zotero-snap/common/Zotero/storage/AV3QJGKK/Antonsanti et al. - 2021 - Partial Matching in the Space of Varifolds.pdf:application/pdf;arXiv.org Snapshot:/home/pierre/snap/zotero-snap/common/Zotero/storage/KQ6APDN7/2103.html:text/html},
}

@article{renMapTreeRecoveringMultiple2020,
