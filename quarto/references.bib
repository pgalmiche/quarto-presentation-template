	keywords = {Breast, Breath-hold, DIBH, Surface-guided radiotherapy},
	pages = {51--57},
	file = {ScienceDirect Full Text PDF:/home/pierre/snap/zotero-snap/common/Zotero/storage/YWRSS7IM/Penninkhof et al. - 2022 - Evaluation of image-guided and surface-guided radi.pdf:application/pdf;ScienceDirect Snapshot:/home/pierre/snap/zotero-snap/common/Zotero/storage/Y8775N7Y/S2405632422000063.html:text/html},
