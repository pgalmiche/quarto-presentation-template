	year = {2019},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/acm2.12700},
	keywords = {optical surface scanning, patient positioning, interfraction motion, surface guided radiotherapy},
	pages = {61--68},
	file = {Full Text PDF:/home/pierre/snap/zotero-snap/common/Zotero/storage/MM24VGMH/Kügele et al. - 2019 - Surface guided radiotherapy (SGRT) improves breast.pdf:application/pdf;Snapshot:/home/pierre/snap/zotero-snap/common/Zotero/storage/9SXQ3FNI/acm2.html:text/html},
