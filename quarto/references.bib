	pages = {1--11},
	file = {Kleiman et al. - 2015 - SHED shape edit distance for fine-grained shape s.pdf:/home/pierre/snap/zotero-snap/common/Zotero/storage/2N99WDEV/Kleiman et al. - 2015 - SHED shape edit distance for fine-grained shape s.pdf:application/pdf},
}

@article{rustamovMapbasedExplorationIntrinsic2013a,
