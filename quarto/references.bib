	keywords = {Computer Science - Computer Vision and Pattern Recognition},
	annote = {Comment: 11 pages, 4 figures},
	file = {arXiv Fulltext PDF:/home/pierre/snap/zotero-snap/common/Zotero/storage/AFHV8J5B/Yang et al. - 2021 - LDDMM-Face Large Deformation Diffeomorphic Metric.pdf:application/pdf;arXiv.org Snapshot:/home/pierre/snap/zotero-snap/common/Zotero/storage/WFP4ETDK/2108.html:text/html},
